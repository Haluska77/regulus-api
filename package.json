{
  "name": "regulus-api",
  "version": "1.0.0",
  "description": "Regulus Rest API",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && npm run format && node ./dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haluska77/Regulus-API.git"
  },
  "keywords": [],
  "author": "Stanislav Haluska",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haluska77/Regulus-API/issues"
  },
  "homepage": "https://github.com/haluska77/Regulus-API#readme",
  "dependencies": {
    "axios": "1.1.3",
    "dotenv-extended": "^2.9.0",
    "express": "^4.21.1",
    "path": "^0.12.7",
    "swagger-ui-express": "^5.0.1",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8",
    "zod-openapi": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^18.13.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/xml2js": "^0.4.14",
    "prettier": "^3.3.3",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5",
    "vitest": "^1.6.0"
  }
